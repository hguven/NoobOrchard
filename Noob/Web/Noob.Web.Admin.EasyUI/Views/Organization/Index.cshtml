<style type="text/css">
    .tree-folder {
        width: 0px;
    }

    .tree-file {
        width: 0px;
    }
</style>
<div data-options="region:'center',border:false" class="divCenter">
    <table id="dg"></table>
    <div id="mm" class="easyui-menu" style="width:120px;">
        <div onclick="createDept()" data-options="iconCls:'icon-add'" style="display:@ViewBag.Create">新增组织机构</div>
        <div onclick="createDeptChild()" data-options="iconCls:'icon-add'" style="display:@ViewBag.Create">新增子组织机构</div>
        <div class="menu-sep"></div>
        <div onclick="updateDept()" style="display:@ViewBag.Edit">编辑组织机构</div>
        <div onclick="deleteDept()" style="display:@ViewBag.Delete">删除组织机构</div>
    </div>
    <div id="tbar" style="text-align:right">
        <a href="javascript:void(0)" onclick="createModule()" class="easyui-linkbutton" data-options="iconCls:'icon-add',plain:true" style="display:@ViewBag.Create">新增</a>
        <a href="javascript:void(0)" onclick="updateDept()" class="easyui-linkbutton" data-options="iconCls:'icon-edit',plain:true" style="display:@ViewBag.Edit">编辑</a>
        <a href="javascript:void(0)" onclick="deleteDept()" class="easyui-linkbutton" data-options="iconCls:'icon-cut',plain:true" style="display:@ViewBag.Delete">删除</a>
    </div>
</div>
<script type="text/javascript" src="@Url.Content(ViewBag.RootUrl + "Scripts/Admin/Admin/Organization.js")"></script>
